<!DOCTYPE html>
<!--[if IE 8]><html class="lt-ie9" lang="en"><![endif]-->
<!--[if gt IE 8]><!--><html lang="en"><!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	{{#if norobots}}
		<meta name="robots" content="noindex,nofollow">
	{{/if}}

	{{> head-opengraph }}

	<!-- build:css(./.tmp) /styles/styles.min.css -->
	<link rel="stylesheet" href="/styles/style.css">
	<!-- endbuild -->
</head>
<body>
	{{> body-gtm }}

	<div class="container-fluid">

		{{> body-header }}

		<div class="row">
			<div class="col-xs-22 col-xs-push-1 col-sm-20 col-sm-push-2 col-md-18 col-md-push-3 col-lg-16 col-lg-push-4">
				{{#if title}}
					<h1>{{title}}</h1>
				{{/if}}

				{% body %}
			</div>
		</div>

		{{> body-footer }}

	</div>
	{{!--
	<!-- build:js(./source) scripts/vendor/jquery.min.js -->
	<script src="scripts/vendor/jquery/jquery.js"></script>
	<!-- endbuild -->

	<!-- build:js(./source) scripts/main.min.js -->
	<script src="scripts/main.js"></script>
	<!-- endbuild -->
	--}}
</body>
</html>
